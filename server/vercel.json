{
    "version": 2,
    "builds": [
      {
        "src": "server.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "functions": {
      "api/users/register": {
        "runtime": "nodejs14.x",
        "timeout": 100 
      }
    }
  }